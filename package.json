{
  "name": "wvus-forms",
  "version": "2.0.0",
  "description": "Validessa - Defender of Data (WVUS Form Validation Library)",
  "repository": {
    "type": "git",
    "url": "https://github.com/wvus-ibu/wvus-forms.git"
  },
  "author": "World Vision, US",
  "license": "UNLICENSED",
  "homepage": "https://github.com/wvus-ibu/wvus-forms",
  "keywords": ["react-component", "react", "forms"],
  "scripts": {
    "prepare":
      "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./dist",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "test": "jest",
    "test-watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "test-storybook": "storyshots"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/addon-info": "^3.2.14",
    "@storybook/addon-links": "^3.2.14",
    "@storybook/addon-storyshots": "^3.2.16",
    "@storybook/addons": "^3.2.16",
    "@storybook/channels": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^4.2.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "prettier": "^1.8.2",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "sinon": "^1.17.3",
    "swing-ui": "github:wvus-ibu/swing#v2.8.2"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^15.0.0",
    "react-dom": "~0.14.8 || ^15.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.10.0",
    "fsevents": "^1.1.1",
    "lodash": "^4.17.4",
    "validator": "^7.0.0"
  }
}
